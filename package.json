  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/EXT-RadioPlayer"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://wiki.bugsounet.fr/en/EXT-RadioPlayer",
  "dependencies": {
    "@magicmirror2/cvlc": "^1.0.8",
    "esbuild": "^0.20.2",
    "glob": "^10.3.12"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@stylistic/eslint-plugin": "^1.7.2",
    "eslint": "^9.1.0",
    "eslint-plugin-import": "^2.29.1"
